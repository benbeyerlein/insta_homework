<div class="page-header">
  <h1>Photos for User <%= current_user.email %></h1>
</div>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Image</th>
      <th>Caption</th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @photo.each do |photo| %>
      <tr>
        <td><img src="<%= photo.image_url %>" width=300></td>
        <td><%= photo.caption %></td>
      </tr>
    <% end %>
  </tbody>
</table>
